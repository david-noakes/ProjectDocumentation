\par\pard\plain \tx240\tx720\li720\fi-360\ri-998\ql\ltrch\loch \ls5\ilvl0{\listtext\f1\fs24\b0\i0\cf1	\bullet 	}{\f1\fs24\b0\i0\cf1 POST /admin/eoi/\{provider\}/verifications/\{verification_id\}/verify}
